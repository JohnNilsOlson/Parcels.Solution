@using Parcels.Models;

<h1>Your Packages</h1>
@if (Model.Count == 0)
{
  <p>There are no items in the list.</p>
}
@foreach (Parcel newParcel in Model)
{ <ul>
    <li>Parcel Volume: @newParcel.Volume() Cubic Ft</li>
    <li>Parcel Weight: @newParcel.Weight Lbs</li>
    <li>Parcel Estimated Cost: $@newParcel.CostToShip().00</li>
  </ul>
}